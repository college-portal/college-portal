{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mykeels/WhatsChanged"
        },
        {
            "type": "path",
            "url": "packages/mykeels/laravel-filters",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "jrm2k6/cloudder": "^0.6.0",
        "laravel/framework": "5.6.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "^1.0",
        "mpociot/laravel-apidoc-generator": "^2.0",
        "tymon/jwt-auth": "0.5.*",
        "mykeels/filters": "dev-feat/package/filters"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "spatie/phpunit-watcher": "^1.5",
        "icyapril/whats-changed": "dev-master"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "gh:deploy": [
            "sh gh-deploy.sh"
        ],
        "watch": [
            "phpunit-watcher watch"
        ],
        "test": [
            "phpunit"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
